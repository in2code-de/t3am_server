{
    "name": "in2code/t3am_server",
    "description": "T3AM Server - TYPO3 Authentication Manager Server",
    "type": "typo3-cms-extension",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Oliver Eglseder",
            "email": "php@vxvr.de"
        },
        {
          "name": "Daniel Hoffmann",
          "email": "daniel.hoffmann@in2code.de"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4 <=8.2",
        "ext-openssl": "*",
        "typo3/cms-core": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "In2code\\T3AM\\Server\\": "Classes"
        }
    },
    "conflict": {
        "in2code/t3am": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        },
        "typo3/cms": {
            "extension-key": "t3am_server"
        }
    }
}
